{
  "name": "api",
  "version": "0.0.1",
  "proxy-disable": "http://localhost:3333",
  "main": "server.js",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "@sentry/node": "^5.7.1",
    "@types/events": "^3.0.0",
    "@types/koa": "^2.11.4",
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "clear": "^0.1.0",
    "cross-env": "^5.2.0",
    "email-templates": "^7.0.4",
    "es6-error": "^4.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "events": "^3.2.0",
    "formidable": "^1.2.1",
    "gulp": "^4.0.2",
    "gulp-livereload": "^4.0.1",
    "gulp-nodemon": "^2.4.2",
    "http-event-stream": "^0.2.0",
    "ioredis": "^4.17.3",
    "knex": "^0.19.1",
    "koa": "^2.7.0",
    "koa-body": "^4.1.0",
    "koa-compose": "^4.1.0",
    "koa-etag": "^3.0.0",
    "koa-helmet": "^5.2.0",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^4.1.3",
    "koa-proxy": "^1.0.0-alpha.3",
    "koa-redis": "^4.0.1",
    "koa-response-time": "^2.1.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-session": "^5.12.3",
    "koa-static": "^5.0.0",
    "koa-timeout-v2": "^1.0.0",
    "koa-validate": "^1.0.7",
    "node-pushnotifications": "^1.4.0",
    "nodemailer": "^6.4.6",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "pg-types": "^3.0.0",
    "sharp": "^0.25.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.1",
    "uuid": "^3.4.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development gulp",
    "prod": "NODE_ENV=production node server.js",
    "migrate:dev": "NODE_ENV=development knex migrate:latest",
    "specs": "npx jest",
    "redis-start": "brew services start redis",
    "redis-stop": "brew services stop redis"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "modulePaths": [
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/coverage/**"
    ],
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(tests|spec))\\.(js|ts)x?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/coverage/"
    ]
  },
  "devDependencies": {
    "chance": "^1.1.6"
  }
}
